{
  "main": "dist/setup.js",
  "private": true,
  "scripts": {
    "build": "babel src -d dist",
    "postbuild": "git add dist",
    "docs": "esdoc",
    "postdocs": "git add docs",
    "lint": "eslint src",
    "start": "node .",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "npm-watch"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "//node_modules|dist/"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "watch": {
    "build": "src/**/*.js",
    "docs": "src/**/*.js",
    "lint": "src/**/*.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "command-line-args": "^4.0.4",
    "esdoc": "^0.5.2",
    "eslint": "^3.19.0",
    "faker": "^4.1.0",
    "jest": "^20.0.3",
    "lodash": "^4.17.4",
    "npm-watch": "^0.1.9",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "command-line-usage": "^4.0.0",
    "shelljs": "^0.7.7"
  }
}
